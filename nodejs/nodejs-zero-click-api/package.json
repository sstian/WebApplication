{
  "name": "nodejs-zero-click-api",
  "version": "0.1.0",
  "description": "Automated provisioning of API",
  "private": true,
  "license": "ISC",
  "homepage": "http://127.0.0.1:3300",
  "index": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "stop": "taskkill -F -IM node.exe",

    "convict-format-with-validator": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "swagger-stats": "^0.99.2",
    "swagger-ui-express": "^4.2.0",
    "winston": "^3.3.3"
  }
}
